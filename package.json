    "test-browser": "node ./bin/test-browser.js",
    "lint": "eslint --ext .js,.ts src test/test.ts",
    "test": "npm run lint && bash ./bin/run-test.sh",
    "build": "tsc && webpack --mode=production",
    "tsc": "tsc --build tsconfig.json",
    "dev": "node ./bin/dev-server.js",
